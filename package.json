{
  "name": "story-que-mon-regne-vienne",
  "version": "1.0.0",
  "description": "A storyline adventure",
  "main": "./story.json",
  "scripts": {
    "compile": "storylines .",
    "chart": "storylines . /tmp/story.json && storylines-chart  /tmp/story.json /tmp/data.json /tmp/data.dot",
    "compile-local": "storylines . > node_modules/storylines/frontend/story-wip.json",
    "local": "npm run run-local & npm run watch-local",
    "watch-local": "nodemon -e js,md node_modules/.bin/storylines . node_modules/storylines/frontend/story-wip.json",
    "run-local": "nodemon node_modules/storylines/frontend.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neamar/story-que-mon-regne-vienne.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Neamar/story-que-mon-regne-vienne/issues"
  },
  "homepage": "https://github.com/Neamar/story-que-mon-regne-vienne#readme",
  "dependencies": {},
  "devDependencies": {
    "nodemon": "^1.17.2",
    "storylines": "^1.3.1"
  }
}
